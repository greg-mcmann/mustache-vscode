{
  "name": "mustache-language",
  "version": "1.0.1",
  "publisher": "gmcmann",
  "displayName": "Mustache Language",
  "description": "Mustache language support for Visual Studio Code",
  "license": "MIT",
  "readme": "README.md",
  "icon": "media/icon.png",
  "keywords": [
    "mustache",
    "language",
    "grammar",
    "syntax",
    "template"
  ],
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "mustache",
        "aliases": [
          "Mustache",
          "mustache"
        ],
        "extensions": [
          ".mustache"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "mustache",
        "scopeName": "text.plain.mustache",
        "path": "./syntaxes/mustache.tmLanguage.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/greg-mcmann/mustache-vscode.git"
  }
}
