{
  "name": "mustache-syntax",
  "displayName": "Mustache",
  "description": "Mustache language support for Visual Studio Code",
  "license": "MIT",
  "version": "1.0.0",
  "readme": "README.md",
  "icon": "media/icon.png",
  "keywords": [
    "mustache",
    "language",
    "grammar",
    "syntax"
  ],
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "mustache",
      "aliases": ["Mustache", "mustache"],
      "extensions": [".mustache"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "mustache",
      "scopeName": "text.plain.mustache",
      "path": "./syntaxes/mustache.tmLanguage.json"
    }]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/greg-mcmann/mustache-vscode.git"
  }
}
